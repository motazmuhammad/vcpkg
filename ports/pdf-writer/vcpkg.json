{
  "name": "pdf-writer",
  "version": "4.6.7",
  "description": "PDF-Writer, Also known as PDFHummus. PDFHummus is a Fast and Free C++ Library for Creating, Parsing an Manipulating PDF Files and Streams",
  "homepage": "https://github.com/galkahana/PDF-Writer",
  "license": "Apache-2.0",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
     {"name":"libpng"}
  ],
  "features": {
    "nopng": { "description": "Excludes support for PNG "},
    "nojpeg": { "description": "Excludes Support for JPEG "},
    "notiff": { "description": "Excludes Support for TIFF "},
    "bundled": { "description": "use bundled dependencies"},
    "fuzz": { "description": "enables fuzz testing"}
  }
}
